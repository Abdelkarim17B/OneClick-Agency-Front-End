{
  "name": "oneclick-agency-frontend",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "OneClick Digital Agency - Creative portfolio website with immersive parallax experience",
  "author": "Abdelkarim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Abdelkarim17B/OneClick-Agency-Front-End.git"
  },
  "bugs": {
    "url": "https://github.com/Abdelkarim17B/OneClick-Agency-Front-End/issues"
  },
  "homepage": "https://github.com/Abdelkarim17B/OneClick-Agency-Front-End#readme",
  "keywords": [
    "react",
    "typescript",
    "vite",
    "parallax",
    "agency",
    "portfolio",
    "responsive",
    "tailwindcss"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 2",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "pre-commit": "./scripts/pre-commit.sh",
    "docker:build": "docker build -t oneclick-agency .",
    "docker:run": "docker run -p 3000:80 oneclick-agency",
    "docker:dev": "docker-compose --profile development up",
    "docker:prod": "docker-compose up -d"
  },
  "dependencies": {
    "@react-spring/parallax": "^9.7.3",
    "lottie-react": "^2.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.3"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/react-lottie": "^1.2.10",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^7.0.5"
  }
}
